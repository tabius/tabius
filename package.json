{
  "name": "tabius",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "repository": {},
  "scripts": {
    "build": "npm run update-version && ng build",
    "build:ssr-dev-ru": "npm run update-version && ng build -c dev-ru --build-optimizer=false && ng run tabius:server",
    "build:ssr-org": "npm run update-version && ng build -c org && ng run tabius:server:org && npm run fix-site-index && npm run inline-css",
    "build:ssr-ru": "npm run update-version && ng build -c ru && ng run tabius:server:ru && npm run inline-css",
    "start-dist": "node dist/server/main.js",
    "start-ng-ssr-ru": "ng run tabius:serve-ssr --port=12101",
    "start-ng-browser-ru": "npm run update-version && ng serve -c dev-ru --port=12102",
    "start-ng-browser-org": "npm run update-version && ng serve -c dev-org --port=13102",
    "backend": "nodemon",
    "fix-site-index": "mv dist/browser/index-org.html dist/browser/index.html",
    "inline-css": "ts-node scripts/inline-css.ts && npm run ngsw-json",
    "ngsw-json": "node_modules/.bin/ngsw-config dist/browser ngsw-config.json",
    "sitemap": "ts-node scripts/generate-sitemap.ts",
    "update-deps": "rm package-lock.json && rm -rf node_modules && npm install",
    "update-version": "node scripts/update-build-version.js",
    "pw-tests-dev-ru": "TEST_ENV=dev-ru playwright test",
    "pw-tests-dev-org": "TEST_ENV=dev-org playwright test",
    "pw-tests-ru": "TEST_ENV=ru playwright test",
    "pw-tests-org": "TEST_ENV=org playwright test",
    "pw-tests-ui-dev-ru": "TEST_ENV=dev-ru playwright test --ui",
    "pw-tests-ui-dev-org": "TEST_ENV=dev-org playwright test --ui",
    "pw-tests-ui-ru": "TEST_ENV=ru playwright test --ui",
    "pw-tests-ui-org": "TEST_ENV=org playwright test --ui",
    "format": "prettier --write \"./**/*.{ts,js,scss}\"",
    "lint": "eslint . --ext .ts,.html",
    "jest": "jest",
    "api-tests": "jest tests/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/pwa": "^18.2.12",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "@angular/ssr": "^18.2.12",
    "@auth0/auth0-angular": "^2.2.3",
    "@nestjs/common": "10.4.15",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@sentry/angular": "8.44.0",
    "assertic": "^1.1.3",
    "aws-jwt-verify": "^4.0.1",
    "body-parser": "1.20.3",
    "core-js": "3.39.0",
    "cors": "2.8.5",
    "express": "4.21.2",
    "frescas": "^0.9.0",
    "hammerjs": "2.0.8",
    "jwks-rsa": "3.1.0",
    "mysql2": "3.11.5",
    "nanoid": "^3.3.8",
    "nosleep.js": "0.12.0",
    "otrie": "^1.1.3",
    "pure-md5": "0.1.14",
    "rxjs": "^7.8.1",
    "schema-dts": "^1.1.2",
    "tslib": "2.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular-eslint/builder": "18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "18.2.13",
    "@angular/language-service": "^18.2.13",
    "@sentry/cli": "2.39.1",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "4.17.21",
    "@types/hammerjs": "^2.0.46",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4"
  }
}