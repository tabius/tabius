{
  "name": "tabius",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "repository": {},
  "scripts": {
    "clean": "rm -rf dist",
    "full-clean": "find . -depth \\( -name \"node_modules\" -o -name \".next\" -o -name \"dist\" -o -name \".angular\" -o -name \"build\" \\) -type d -exec rm -rf {} +",
    "build": "npm run update-version && ng build",
    "build:ssr-dev-ru": "npm run update-version && ng build -c dev-ru --build-optimizer=false && ng run tabius:server",
    "build:ssr-ru": "npm run clean && npm run update-version && ng build -c ru && ng run tabius:server:ru && npm run inline-css",
    "deploy:ssr-ru": "npm run build:ssr-ru && rsync -avz -e ssh ./dist/ ram:work/tabius-ru/dist",
    "start-dist": "node dist/server/main.js",
    "start-ng-ssr-ru": "ng run tabius:serve-ssr --port=12101",
    "start-ng-browser-ru": "npm run update-version && ng serve -c dev-ru --port=12102",
    "backend": "nodemon",
    "fix-site-index": "mv dist/browser/index-org.html dist/browser/index.html",
    "inline-css": "ts-node scripts/inline-css.ts && npm run ngsw-json",
    "ngsw-json": "node_modules/.bin/ngsw-config dist/browser ngsw-config.json",
    "sitemap": "ts-node scripts/generate-sitemap.ts",
    "update-deps": "rm package-lock.json && rm -rf node_modules && npm install",
    "update-version": "node scripts/update-build-version.js",
    "pw-tests-dev-ru": "TEST_ENV=dev-ru playwright test",
    "pw-tests-ru": "TEST_ENV=ru playwright test",
    "pw-tests-ui-dev-ru": "TEST_ENV=dev-ru playwright test --ui",
    "pw-tests-ui-ru": "TEST_ENV=ru playwright test --ui",
    "format": "prettier --write \"./**/*.{ts,js,scss}\"",
    "lint": "eslint . --ext .ts,.html",
    "jest": "jest",
    "api-tests": "jest tests/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/platform-server": "^19.2.14",
    "@angular/pwa": "^19.2.15",
    "@angular/router": "^19.2.14",
    "@angular/service-worker": "^19.2.14",
    "@angular/ssr": "^19.2.15",
    "@auth0/auth0-angular": "^2.2.3",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@sentry/angular": "^9.30.0",
    "assertic": "^1.3.0",
    "aws-jwt-verify": "^4.0.1",
    "body-parser": "^2.2.0",
    "core-js": "^3.43.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "frescas": "^0.9.0",
    "hammerjs": "^2.0.8",
    "jwks-rsa": "^3.2.0",
    "mysql2": "^3.14.1",
    "nanoid": "^3.3.11",
    "nosleep.js": "^0.12.0",
    "otrie": "^1.2.1",
    "pure-md5": "0.1.14",
    "rxjs": "^7.8.2",
    "schema-dts": "^1.1.5",
    "node-telegram-bot-api": "^0.66.0",
    "tslib": "^2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular-eslint/builder": "^19.8.1",
    "@angular-eslint/eslint-plugin": "^19.8.1",
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular-eslint/schematics": "^19.8.1",
    "@angular-eslint/template-parser": "^19.8.1",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@angular/language-service": "^19.2.14",
    "@sentry/cli": "^2.46.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "4.17.21",
    "@types/hammerjs": "^2.0.46",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@ungap/structured-clone": "^1.3.0",
    "browser-sync": "^3.0.4",
    "eslint": "^8.57.1",
    "fake-indexeddb": "^6.0.1",
    "http-server": "^14.1.1",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}