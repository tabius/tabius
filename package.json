{
  "name": "tabius",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "repository": {},
  "scripts": {
    "build": "npm run update-version && ng build",
    "build:ssr-dev-ru": "npm run update-version && ng build -c dev-ru && ng run tabius:server",
    "build:ssr-org": "npm run update-version && ng build -c org && ng run tabius:server:org && npm run fix-org-index && npm run inline-css",
    "build:ssr-ru": "npm run update-version && ng build -c ru && ng run tabius:server:ru && npm run inline-css",
    "run:dev-serve-ssr": "ng run tabius:serve-ssr --port=12101",
    "e2e": "ng e2e",
    "fix-org-index": "mv dist/browser/index-org.html dist/browser/index.html",
    "inline-css": "ts-node scripts/inline-css.ts && npm run ngsw-json",
    "lint": "eslint . --ext .ts",
    "ngsw-json": "node_modules/.bin/ngsw-config dist/browser ngsw-config.json",
    "serve:ssr": "node dist/server/main.js",
    "server": "nodemon",
    "server-session-debug": "DEBUG=express-session nodemon",
    "sitemap": "ts-node scripts/generate-sitemap.ts",
    "start-ru": "npm run update-version && ng serve -c dev-ru --port=12102",
    "start-org": "npm run update-version && ng serve -c dev-org --port=13102",
    "start-ssr-ru": "npm run build:ssr-dev-ru && npm run serve:ssr",
    "start-ssr-org": "npm run build:ssr-dev-org && npm run serve:ssr",
    "update-version": "node scripts/update-build-version.js",
    "update-deps": "rm package-lock.json && rm -rf node_modules && npm install",
    "pw-tests": "playwright test",
    "pw-tests-dev": "playwright test --ui"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/cdk": "^16.2.2",
    "@angular/common": "^16.2.3",
    "@angular/compiler": "^16.2.3",
    "@angular/core": "^16.2.3",
    "@angular/forms": "^16.2.3",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/platform-server": "^16.2.3",
    "@angular/pwa": "^16.2.1",
    "@angular/router": "^16.2.3",
    "@angular/service-worker": "^16.2.3",
    "@auth0/auth0-angular": "2.2.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.2.3",
    "@nestjs/core": "10.2.3",
    "@nestjs/platform-express": "10.2.3",
    "@nguniversal/common": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@sentry/angular-ivy": "7.66.0",
    "@sentry/tracing": "7.66.0",
    "assertic": "0.9.4",
    "async-mutex": "0.4.0",
    "auth0": "3.7.0",
    "aws-jwt-verify": "^4.0.0",
    "body-parser": "1.20.2",
    "core-js": "3.32.1",
    "cors": "2.8.5",
    "domino-ext": "2.1.4",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "frescas": "^0.9.0",
    "hammerjs": "2.0.8",
    "jwks-rsa": "3.0.1",
    "mysql2": "3.6.0",
    "nanoid": "^3.3.6",
    "nosleep.js": "0.12.0",
    "pure-md5": "0.1.14",
    "rxjs": "7.8.1",
    "schema-dts": "1.1.2",
    "svg4everybody": "2.1.9",
    "tslib": "2.6.2",
    "validator": "13.11.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/language-service": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@sentry/cli": "2.20.5",
    "@types/auth0": "3.3.3",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/hammerjs": "2.0.41",
    "@types/jasmine": "4.3.5",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.17.5",
    "@types/validator": "13.11.1",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.47.0",
    "http-server": "^14.1.1",
    "jasmine-core": "5.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "nodemon": "3.0.1",
    "@playwright/test": "^1.37.1",
    "sass": "1.65.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
