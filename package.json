{
  "name": "tabius",
  "version": "1.0.0",
  "license": "GPL-3.0-only",
  "repository": {},
  "scripts": {
    "build": "npm run update-version && ng build",
    "build:ssr-dev-ru": "npm run update-version && ng build -c dev-ru --build-optimizer=false && ng run tabius:server",
    "build:ssr-org": "npm run update-version && ng build -c org && ng run tabius:server:org && npm run fix-site-index && npm run inline-css",
    "build:ssr-ru": "npm run update-version && ng build -c ru && ng run tabius:server:ru && npm run inline-css",
    "start-dist": "node dist/server/main.js",
    "start-ng-ssr-ru": "ng run tabius:serve-ssr --port=12101",
    "start-ng-browser-ru": "npm run update-version && ng serve -c dev-ru --port=12102",
    "start-ng-browser-org": "npm run update-version && ng serve -c dev-org --port=13102",
    "backend": "nodemon",
    "fix-site-index": "mv dist/browser/index-org.html dist/browser/index.html",
    "inline-css": "ts-node scripts/inline-css.ts && npm run ngsw-json",
    "ngsw-json": "node_modules/.bin/ngsw-config dist/browser ngsw-config.json",
    "sitemap": "ts-node scripts/generate-sitemap.ts",
    "update-deps": "rm package-lock.json && rm -rf node_modules && npm install",
    "update-version": "node scripts/update-build-version.js",
    "pw-tests-dev-ru": "TEST_ENV=dev-ru playwright test",
    "pw-tests-dev-org": "TEST_ENV=dev-org playwright test",
    "pw-tests-ru": "TEST_ENV=ru playwright test",
    "pw-tests-org": "TEST_ENV=org playwright test",
    "pw-tests-ui-dev-ru": "TEST_ENV=dev-ru playwright test --ui",
    "pw-tests-ui-dev-org": "TEST_ENV=dev-org playwright test --ui",
    "pw-tests-ui-ru": "TEST_ENV=ru playwright test --ui",
    "pw-tests-ui-org": "TEST_ENV=org playwright test --ui",
    "format": "prettier --write \"./**/*.{ts,js,scss}\"",
    "lint": "eslint . --ext .ts,.html",
    "jest": "jest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/platform-server": "^17.1.1",
    "@angular/pwa": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@angular/service-worker": "^17.1.1",
    "@angular/ssr": "^17.1.1",
    "@auth0/auth0-angular": "^2.2.3",
    "@nestjs/common": "10.3.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/platform-express": "10.3.1",
    "@sentry/angular-ivy": "7.98.0",
    "@sentry/tracing": "7.98.0",
    "assertic": "^1.1.1",
    "async-mutex": "0.4.1",
    "aws-jwt-verify": "^4.0.0",
    "body-parser": "1.20.2",
    "core-js": "3.35.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "frescas": "^0.9.0",
    "hammerjs": "2.0.8",
    "jwks-rsa": "3.1.0",
    "mysql2": "3.9.0",
    "nanoid": "^3.3.6",
    "nosleep.js": "0.12.0",
    "otrie": "^1.1.3",
    "pure-md5": "0.1.14",
    "rxjs": "7.8.1",
    "schema-dts": "1.1.2",
    "tslib": "2.6.2",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "^17.2.1",
    "@angular-eslint/eslint-plugin-template": "^17.2.1",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "^17.2.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@angular/language-service": "^17.1.1",
    "@playwright/test": "^1.41.1",
    "@sentry/cli": "2.26.0",
    "@types/cookie-parser": "1.4.6",
    "@types/express": "4.17.21",
    "@types/hammerjs": "2.0.45",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}