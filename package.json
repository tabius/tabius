    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "@nguniversal/common": "v15-lts",
    "@nguniversal/express-engine": "v15-lts",
    "@sentry/angular-ivy": "7.61.1",
    "@sentry/tracing": "7.61.1",
    "assertic": "0.9.2",
